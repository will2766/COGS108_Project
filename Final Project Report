   "source": [
    "\n",
    "# Dictionary to hold feature names and their corresponding R^2 scores\n",
    "r2_scores = {}\n",
    "\n",
    "for feature in df.columns.drop('cardio'):\n",
    "    X = df[[feature]]\n",
    "    \n",
    "    # Create a linear regression model\n",
    "    model = LinearRegression()\n",
    "    \n",
    "    model.fit(X, y)\n",
    "    \n",
    "    y_pred = model.predict(X)\n",
    "    \n",
    "    r2_scores[feature] = r2_score(y, y_pred)\n",
    "\n",
    "for feature, score in r2_scores.items():\n",
    "    print(f\"{feature}: R^2 score = {score}\")"
   ]
